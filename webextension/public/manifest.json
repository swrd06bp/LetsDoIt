{
  "manifest_version": 2,
  "name": "LetsDoIt",
  "author": "Boris",
  "version": "1.0.5",
  "description": "Best to do list to help you acheive your goals",
  "icons": {
    "16": "logo2.png",
    "48": "logo2.png",
    "128": "logo2.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Lets Do It"
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>",
    "https://api.lets-do-it.me/*",
    "https://app.lets-do-it.me/*"
   ]
}
